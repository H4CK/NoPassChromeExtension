{
  "manifest_version": 2,

  "name": "NoPass",
  "description": "Universal password less authentication system.",
  "version": "1.0",

  "permissions": [
    "tabs"
  ],
  "background": {
        "scripts": [
            "script.js"
        ]
    },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}